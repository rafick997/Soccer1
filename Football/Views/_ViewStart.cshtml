@{
    if (this.User.IsInRole("Admin"))
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else if (this.User.IsInRole("User"))
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else if (!this.User.Identity.IsAuthenticated)
    {
        Layout = "~/Views/Shared/_LayoutNoAuthorized.cshtml";
    }

}
